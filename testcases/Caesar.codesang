karya void caesarEncrypt(num: p0, num: p1, num: p2):
  keliye (num i=0; i<p1; i++) {
    num temp = load(p0 + i*4)
    temp = (temp + p2) % 26
    store(p0 + i*4, temp)
  }
}

karya void caesarDecrypt(num: p0, num: p1, num: p2):
  keliye (num i=0; i<p1; i++) {
    num temp = load(p0 + i*4)
    temp = (temp - p2) % 26
    store(p0 + i*4, temp)
  }
}